		"ace-builds": "^1.7.1",
		"antd": "^4.22.4",
		"classnames": "^2.3.1",
		"color": "^4.2.1",
		"framer-motion": "^6.4.2",
		"fuse.js": "^6.4.6",
		"jsonpath-plus": "^7.0.0",
		"mark.js": "^8.11.1",
		"moment": "^2.29.1",
		"prismjs": "^1.27.0",
		"react-ace": "^10.1.0",
		"react-diff-viewer": "^3.1.1",
		"react-helmet": "^6.1.0",
		"react-hook-form": "^7.32.2",
		"react-jss": "~10.5.1",
		"react-markdown": "^8.0.0",
		"react-scripts": "^5.0.0",
		"react-scroll": "^1.8.5",
		"remark-gfm": "^3.0.1",
		"uuid": "^8.3.2"
	},
	"scripts": {
		"api-models:generate": "rm -rf src/components/api/$npm_config_output_folder; mkdir -p src/components/api; cp -R $npm_config_input_folder/model src/components/api/$npm_config_output_folder; rm -rf $npm_config_input_folder; cd src/components/api/$npm_config_output_folder; mv models.ts index.ts; prettier --write .",
		"api-models:global": "npm run api-models:generate --input_folder=globalApi --output_folder=global",
		"codegen": "node codegen.js",
		"start": "npm run storybook",
		"build": "rollup -c rollup.config.ts",
		"storybook": "start-storybook -p 9009 -s public",
		"build-storybook": "build-storybook -s public",
		"test": "react-scripts test --watchAll=false",
		"test-watch": "react-scripts test --watchAll",
		"test-coverage": "npm test -- --coverage --watchAll=false",
		"lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\""
	},
	"main": "lib/index.cjs.js",
	"module": "lib/index.esm.js",
	"types": "lib/types",
	"eslintConfig": {
		"extends": "react-app"
	},
	"jest": {
		"coveragePathIgnorePatterns": [
			".stories.tsx",
			"fixtures/*",
			"types.ts",
			"components/index.ts"
		],
		"resetMocks": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^22.0.1",
		"@rollup/plugin-image": "^2.1.1",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@storybook/addon-actions": "^6.5.9",
		"@storybook/addon-cssresources": "^6.2.9",
		"@storybook/addon-essentials": "^6.5.9",
		"@storybook/addon-links": "^6.5.9",
		"@storybook/addon-storyshots": "^6.5.9",
		"@storybook/addons": "^6.5.9",
		"@storybook/builder-webpack5": "^6.5.9",
		"@storybook/manager-webpack5": "^6.5.9",
		"@storybook/preset-create-react-app": "^4.1.2",
		"@storybook/react": "^6.5.9",
		"@storybook/theming": "^6.5.9",
		"@svgr/rollup": "^6.1.2",
		"@testing-library/react": "^12.1.3",
		"@testing-library/react-hooks": "^7.0.0",
		"@types/color": "^3.0.1",
		"@types/enzyme": "^3.10.8",
		"@types/jest": "^27.4.1",
		"@types/lodash": "^4.14.170",
		"@types/mark.js": "^8.11.5",
		"@types/node": "^17.0.36",
		"@types/prismjs": "^1.16.6",
		"@types/react": "^17.0.9",
		"@types/react-dom": "^17.0.6",
		"@types/react-helmet": "^6.1.1",
		"@types/react-scroll": "^1.8.3",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^5.30.5",
		"@typescript-eslint/parser": "^5.30.5",
		"@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
		"chromatic": "^6.7.0",
		"enzyme": "^3.11.0",
		"eslint-plugin-react": "^7.30.1",
		"normalize.css": "^8.0.1",
		"prettier": "^2.7.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-test-renderer": "^17.0.2",
		"rollup": "^2.75.7",
		"rollup-plugin-styles": "^4.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.32.1",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4"
	},
	"peerDependencies": {
		"react": ">=16.13 || ^17",
		"react-dom": ">=16.13 || ^17"
	}
}
