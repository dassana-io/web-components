{
	"name": "@dassana-io/web-components",
	"version": "0.10.15",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/dassana-io"
	},
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com:dassana-io/web-components",
		"directory": "@dassana-io/web-components"
	},
	"dependencies": {
		"@ant-design/icons": "^4.2.2",
		"@dassana-io/web-utils": "^0.8.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.32",
		"@fortawesome/free-solid-svg-icons": "^5.15.1",
		"@fortawesome/pro-light-svg-icons": "^5.15.3",
		"@fortawesome/pro-regular-svg-icons": "^5.15.3",
		"@fortawesome/react-fontawesome": "^0.1.14",
		"antd": "~4.12.3",
		"bytes": "^3.1.0",
		"classnames": "^2.2.6",
		"color": "^3.1.3",
		"framer-motion": "^2.9.5",
		"fuse.js": "^6.4.2",
		"jsonpath-plus": "^5.0.7",
		"lodash": "^4.17.20",
		"mark.js": "^8.11.1",
		"moment": "^2.29.1",
		"npm": "^6.14.11",
		"prettier": "^2.2.1",
		"prismjs": "^1.23.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-hook-form": "^6.15.4",
		"react-jss": "~10.5.1",
		"react-markdown": "^6.0.1",
		"react-scripts": "^4.0.3",
		"remark-gfm": "^1.0.0",
		"uuid": "^8.3.2"
	},
	"scripts": {
		"api-models:generate": "rm -rf src/components/api/$npm_config_outputFolder; mkdir -p src/components/api; cp -R $npm_config_inputFolder/model src/components/api/$npm_config_outputFolder; rm -rf $npm_config_inputFolder; cd src/components/api/$npm_config_outputFolder; mv models.ts index.ts; prettier --write .",
		"api-models:global": "npm run api-models:generate --inputFolder=globalApi --outputFolder=global",
		"codegen": "node codegen.js",
		"start": "npm run storybook",
		"build": "rollup -c rollup.config.ts",
		"storybook": "start-storybook -p 9009 -s public",
		"build-storybook": "build-storybook -s public",
		"test": "react-scripts test --watchAll=false",
		"test-watch": "react-scripts test --watchAll",
		"test-coverage": "npm test -- --coverage --watchAll=false",
		"lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\""
	},
	"main": "lib/index.cjs.js",
	"module": "lib/index.esm.js",
	"types": "lib/types",
	"eslintConfig": {
		"extends": "react-app"
	},
	"jest": {
		"coveragePathIgnorePatterns": [
			".stories.tsx",
			"fixtures/*",
			"types.ts",
			"components/index.ts"
		],
		"resetMocks": false
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^17.1.0",
		"@rollup/plugin-image": "^2.0.6",
		"@rollup/plugin-node-resolve": "^11.2.0",
		"@storybook/addon-actions": "^6.1.21",
		"@storybook/addon-cssresources": "^6.1.21",
		"@storybook/addon-essentials": "^6.1.21",
		"@storybook/addon-links": "^6.1.21",
		"@storybook/addon-storyshots": "^6.1.21",
		"@storybook/addons": "^6.1.21",
		"@storybook/preset-create-react-app": "^3.1.6",
		"@storybook/react": "^6.1.21",
		"@storybook/theming": "^6.1.21",
		"@svgr/rollup": "^5.5.0",
		"@testing-library/react": "^11.2.5",
		"@testing-library/react-hooks": "^5.1.0",
		"@types/bytes": "^3.1.0",
		"@types/classnames": "^2.2.10",
		"@types/color": "^3.0.1",
		"@types/enzyme": "^3.10.7",
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/jest": "^26.0.15",
		"@types/lodash": "^4.14.162",
		"@types/mark.js": "^8.11.5",
		"@types/node": "^14.14.2",
		"@types/prismjs": "^1.16.3",
		"@types/react": "^17.0.2",
		"@types/react-dom": "^17.0.1",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.16.1",
		"@typescript-eslint/parser": "^4.16.1",
		"@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
		"chromatic": "^5.6.1",
		"enzyme": "^3.11.0",
		"eslint-plugin-react": "^7.22.0",
		"normalize.css": "^8.0.1",
		"react-test-renderer": "^17.0.1",
		"rollup": "^2.40.0",
		"rollup-plugin-styles": "^3.11.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.30.0",
		"tslib": "^2.1.0",
		"typescript": "^4.2.3"
	},
	"peerDependencies": {
		"react": ">=16.13 || ^17",
		"react-dom": ">=16.13 || ^17"
	}
}
